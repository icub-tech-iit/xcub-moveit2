<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="iCub">
    <xacro:arg name="initial_positions_file" default="initial_positions.yaml" />

    <!-- Import iCub urdf file -->
    <xacro:include filename="$(find iCub)/robots/iCubGazeboV3/model.urdf" />

    <!-- Import control_xacro -->
    <xacro:include filename="iCub.ros2_control.xacro" />


    <xacro:iCub_ros2_control name="FakeSystem" initial_positions_file="$(arg initial_positions_file)"/>

</robot>
